@model SCE_Website.ViewModel.CourseViewModel
@using SCE_Website.Models
@{
    ViewBag.Title = "FaAdmin";
}

    <form action="Menu" method="post">
        <input id="AddNewCourse" type="submit" value="AddNewCourse" name="AddNewCourse" />
        <input id="AssignStudents" type="submit" value="AssignStudents" name="AssignStudents" />
        <input id="CourseList" type="submit" value="CourseList" name="CourseList" />
        <input id="ChangeSchedule" type="submit" value="ChangeSchedule" name="ChangeSchedule" />
    </form>

<table class="table table-hover">
    <thead>
        <tr class="table-primary">
            <th scope="col">Course Name</th>
            <th scope="col">Day</th>
            <th scope="col">Hour</th>
            <th scope="col">Classroom</th>
            <th scope="col">Lecturer ID</th>
        </tr>
    </thead>
    @{
        foreach (Course obj in Model.Courses)
        {
            <tbody>
                <tr>
                    <td>@obj.CourseName</td>
                    <td>@obj.Day</td>
                    <td>@obj.Hour</td>
                    <td>@obj.Classroom</td>
                    <td>@obj.LecturerID</td>
                </tr>
            </tbody>
        }

    }
</table>